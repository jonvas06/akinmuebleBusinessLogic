{
  "name": "akinmueble_business_logic",
  "version": "0.0.1",
  "description": "business logic ms",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": "14 || 16 || 18 || 19"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "lint": "npm run eslint && npm run prettier:check",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run rebuild",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "npm run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "docker:build": "docker build -t akinmueble_business_logic .",
    "docker:run": "docker run -p 3000:3000 -d akinmueble_business_logic",
    "premigrate": "npm run build",
    "migrate": "node ./dist/migrate",
    "preopenapi-spec": "npm run build",
    "openapi-spec": "node ./dist/openapi-spec",
    "prestart": "npm run rebuild",
    "start": "node -r source-map-support/register .",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache",
    "rebuild": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "Jon Vasquez <jhonvasquezz2002@gmail.com>",
  "license": "",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/authentication": "^9.0.9",
    "@loopback/boot": "^5.0.9",
    "@loopback/core": "^4.0.9",
    "@loopback/repository": "^5.1.4",
    "@loopback/rest": "^12.0.9",
    "@loopback/rest-explorer": "^5.0.9",
    "@loopback/service-proxy": "^5.0.9",
    "date-fns": "^2.29.3",
    "dotenv": "^16.0.3",
    "loopback-connector-mysql": "^5.4.4",
    "multer": "^1.4.5-lts.1",
    "mysql": "^2.18.1",
    "node-fetch": "^2.6.9",
    "parse-bearer-token": "^1.0.1",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@loopback/build": "^9.0.9",
    "@loopback/eslint-config": "^13.0.9",
    "@loopback/testlab": "^5.0.9",
    "@types/multer": "^1.4.7",
    "@types/node": "^14.18.36",
    "eslint": "^8.35.0",
    "source-map-support": "^0.5.21",
    "typescript": "~4.9.4"
  }
}
